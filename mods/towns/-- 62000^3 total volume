function arcGIS.set_origin(lat, long)
	arcGIS.origin.latitude = lat
	arcGIS.origin.longitude = long
end

function arcGIS.set_scale(scale)
	arcGIS.scale = scale  -- 2 is 1:2 is 16x16 nodes per arc-second
end



